// hashing basic
#include <iostream>
using namespace std;
/*
int arr[10^7]
bool arr[10^8]

*/
int main() {
    int n; // size of array
    cout<<"enter size: "<<endl;
    cin>>n;
    int arr[n];
    /* here inside main
    int arr[10^6] at max $segmentation fault
    bool arr[10^7] 
    */
    
    cout<<"Enter the array elements: "<<endl;
    for(int i = 0; i < n; i++){
        cin>>arr[i];
    }
    
    
    // pre computation
    int hash[13] = {0} ; // when 1 to 12 can be there
    for(int i = 0; i < n; i++){
        hash[arr[i]] += 1;
    }// element array[i] is index here in my hash array
    
    
    
    int q; // no of query
    cin>> q;
    while(q--){
        int number; // take the query number
        cin>>number;
        cout<<hash[number]<<" ";
        //fetching 
    }
    return 0;
}